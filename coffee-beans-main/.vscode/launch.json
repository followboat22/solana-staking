{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch React App",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/Frontend",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["start"],
      "env": {
        "NODE_OPTIONS": "--openssl-legacy-provider"
      },
      "timeout": 30000,
      "restart": true,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
